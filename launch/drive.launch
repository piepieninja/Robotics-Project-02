<launch>
    <node pkg="pr2_teleop" type="teleop_pr2_keyboard" name="spawn_teleop_keyboard" output="screen">
      <remap from="cmd_vel" to="base_controller/command" />

      <param name="walk_vel" value="100.0" />
      <param name="run_vel" value="20.0" />
      <param name="yaw_rate" value="5.0" />
      <param name="yaw_run_rate" value="5.0" />
    </node>
</launch>
